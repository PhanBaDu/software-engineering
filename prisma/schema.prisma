generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Gender {
  MALE
  FEMALE
  OTHER
}

model User {
  id       Int    @id @default(autoincrement())
  fullName String
  email    String @unique
  password String?
  avatarUrl   String?
  phoneNumber String?
  gender Gender?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
